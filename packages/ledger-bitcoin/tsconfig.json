{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "lib",
    "noImplicitAny": false,
    "noImplicitThis": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
  },
  "include": ["src/**/*.ts"],
  "exclude": [
    "**/__tests__/*"
  ],
}